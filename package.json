{
  "name": "restaurant-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "accessibility:test": "axe --dir ./out --include '*.html' --reporter json --output ./accessibility-report.json",
    "accessibility:dev": "next dev --turbopack & sleep 5 && axe http://localhost:3000 --reporter json --output ./accessibility-report.json",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "analyze:both": "BUNDLE_ANALYZE=both next build",
    "security:audit": "npm audit && audit-ci --config audit-ci.json",
    "security:scan": "retire --path . --outputformat json --outputpath retire-report.json",
    "security:check": "npm run security:audit && npm run security:scan",
    "security:headers": "node scripts/check-security-headers.js"
  },
  "dependencies": {
    "@axe-core/react": "^4.10.2",
    "@headlessui/react": "^2.2.7",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@simplewebauthn/browser": "^9.0.1",
    "@simplewebauthn/server": "^9.0.3",
    "@tanstack/react-table": "^8.21.3",
    "@types/bcryptjs": "^2.4.6",
    "argon2": "^0.31.2",
    "axe-core": "^4.10.2",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "cookie": "^1.0.1",
    "critters": "^0.0.23",
    "csrf": "^3.1.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.0.6",
    "focus-trap-react": "^10.3.0",
    "helmet": "^8.0.0",
    "iron-session": "^8.0.3",
    "jose": "^5.9.6",
    "limiter": "^2.1.0",
    "lucide-react": "^0.540.0",
    "next": "15.5.0",
    "next-auth": "^4.24.5",
    "next-safe-action": "^7.9.0",
    "rate-limiter-flexible": "^5.0.0",
    "react": "19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-focus-lock": "^2.13.2",
    "react-hotkeys-hook": "^4.6.1",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "validator": "^13.12.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.15",
    "@types/cookie": "^0.6.0",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "audit-ci": "^7.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-security": "^3.0.1",
    "prettier": "^3.6.2",
    "retire": "^5.2.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "web-vitals": "^4.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
